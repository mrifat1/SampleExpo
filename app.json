{
    "expo": {
        "name": "SampleExpo",
        "slug": "SampleExpo",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/mainIcon.png",
        "scheme": "sampleexpo",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "bundleIdentifier": "com.mahmudrifat.sampleexpo",
            "supportsTablet": true
        },
        "android": {
            "package": "com.mahmudrifat.sampleexpo",
            "googleServicesFile": "./google-services.json",
            "icon": "./assets/images/mainIcon.png",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/mainIcon.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "jsEngine": "hermes"
        },
        "web": {
            "bundler": "metro",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-notifications", [
                "expo-splash-screen",
                {
                    "image": "./assets/images/mainIcon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ]
        ],
        "extra": {
            "eas": {
                "projectId": "c70fc672-b596-4ec4-b796-78930d60ec35"
            }
        },
        "owner": "mahmudrifat"
    }
}